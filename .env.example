# Milvus
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION=clinical_documents

# Elasticsearch
ES_HOST=localhost
ES_PORT=9200
ES_INDEX=clinical_documents

# vLLM
VLLM_BASE_URL=http://localhost:8000
VLLM_MODEL=deepseek-ai/DeepSeek-R1-Distill-Qwen-32B

# Embedding Model
EMBEDDING_MODEL=BAAI/bge-m3
EMBEDDING_DIMENSION=1024

# Reranker
RERANKER_MODEL=BAAI/bge-reranker-v2-m3
RERANK_TOP_K=5

# Search
SEARCH_TOP_K=20
HYBRID_ALPHA=0.5

# EMR Database (Patient MCP)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=emr
DB_USER=
DB_PASSWORD=

# Logging
LOG_LEVEL=INFO
